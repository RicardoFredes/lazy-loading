!function(){var t=document.querySelectorAll("img[data-src]");function e(t){if(t[0].isIntersecting){var a=t[0].target;"loaded"===a.getAttribute("data-status")&&a.classList.remove("Loading")}}window.addEventListener("load",function(){t.forEach(function(t){t.classList.add("LazyLoading","Loading"),t.setAttribute("src",t.getAttribute("data-src")),t.setAttribute("data-status","loading"),t.onload=function(){t.setAttribute("data-status","loaded")};var a=new IntersectionObserver(e);a.observe(t)})})}();